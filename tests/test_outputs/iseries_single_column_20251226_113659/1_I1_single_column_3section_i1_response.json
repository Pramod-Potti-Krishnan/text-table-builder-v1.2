le=\"font-size: 21px;\n               line-height: 1.28;\n               color: var(--text-secondary, #374151);\n               margin: 0;\n               padding-left: 24px;\n               list-style-type: disc;\">\n      <li style=\"margin-bottom: 10px;\">\n        Upskill workforce with new digital competencies and tools.\n      </li>\n      <li style=\"margin-bottom: 10px;\">\n        Foster a culture of innovation, agility, and continuous learning.\n      </li>\n      <li style=\"margin-bottom: 10px;\">\n        Provide targeted training for new systems and optimized processes.\n      </li>\n      <li style=\"margin-bottom: 0;\">\n        Drive change management initiatives for smooth adoption.\n      </li>\n    </ul>\n  </div>\n\n</div>\n","image_url":"https://eshvntffcestlfuofwhv.supabase.co/storage/v1/object/public/generated-images/generated/fcc72618-8a1f-4bc9-a56e-acd94d1383ef_cropped.png?","image_fallback":false,"background_color":"#ffffff","metadata":{"layout_type":"I1","slide_number":1,"image_position":"left","image_dimensions":{"width":660,"height":1080},"image_aspect_ratio":"11:18","content_dimensions":{"width":1200,"height":840},"visual_style":"illustrated","content_style":"bullets","generation_time_ms":12748,"validation":{"valid":true,"template_based":true,"template_path":"app/templates/single_column/single_column_3section_c1_themed.html","variant_id":"single_column_3section_i1"},"multi_step":{"enabled":false},"generation_mode":"single_step"}}
